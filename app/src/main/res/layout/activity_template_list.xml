<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="5dp"
    tools:context="com.cantekin.aquareef.ui.TemplateListActivity">

    <LinearLayout
        android:id="@+id/fvrLayoutRow"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/gray_very_light"
            android:padding="5dp"
            android:text="@string/varsayilan"
            android:textSize="18sp"
            android:textStyle="bold" />

        <ListView
            android:id="@+id/fvrDefaultlists"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:background="@color/gray_very_light"
            android:padding="5dp"
            android:text="@string/kaydedilenler"
            android:textSize="18sp"
            android:textStyle="bold" />

        <ListView
            android:id="@+id/fvrFavoritLists"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </LinearLayout>
</FrameLayout>
